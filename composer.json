{
    "name": "lisaldo/teluino",
    "description": "Um bot do telegram pra brincar com meu arduino",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Teluino\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teluino\\UnitTest\\": "test/unit",
            "Teluino\\IntegrationTest\\": "test/integration"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "require": {
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "vlucas/phpdotenv": "^4.1"
    }
}
